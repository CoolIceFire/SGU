#include <cstdio>
#include <cstring>
#include <cstdlib>
#include <vector>
#include <iostream>
#include <string>
#include <algorithm>
#include <map>
#include <cmath>
#include <ctime>
#include <set>
#include <stack>
#define INF 1000010

using namespace std;

int main()
{
	double s, p;

	while (~scanf("%lf%lf", &s, &p))
	{
		if (p <= s) puts("0");
		else if (p-s < 300.0) puts("1");
		else if (p-s < 900.0) puts("2");
		else if (p-s < 1800.0) puts("3");
		else puts("4");
	}

	return 0;
}
